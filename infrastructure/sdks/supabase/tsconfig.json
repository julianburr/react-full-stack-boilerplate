{
  "compilerOptions": {
    "outDir": "bin",
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "stripInternal": true,
    "experimentalDecorators": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  },
  "files": [
    "apikey.ts",
    "branch.ts",
    "config/index.ts",
    "config/vars.ts",
    "getApikeys.ts",
    "getBranch.ts",
    "getPooler.ts",
    "index.ts",
    "project.ts",
    "provider.ts",
    "settings.ts",
    "types/index.ts",
    "types/input.ts",
    "types/output.ts",
    "utilities.ts"
  ]
}
